veCategory: null\n        }\n    },\n    components: {\n        CourseItem\n    },\n    async mounted() {\n        console.log('mounted')\n\n        document.title = 'Courses | StudyNet'\n\n        await axios\n            .get('courses/get_categories/')\n            .then(response => {\n                console.log(response.data)\n\n                this.categories = response.data\n            })\n        \n        this.getCourses()\n    },\n    methods: {\n        setActiveCategory(category) {\n            console.log(category)\n            this.activeCategory = category\n\n            this.getCourses()\n        },\n        getCourses() {\n            let url = 'courses/'\n\n            if (this.activeCategory) {\n                url += '?category_id=' + this.activeCategory.id\n            }\n\n            axios\n                .get(url)\n                .then(response => {\n                    console.log(response.data)\n\n                    this.courses = response.data\n                })\n        }\n    }\n}\n</script>\n\n<style scoped src=\"../assets/css/Courses.css\">\n</style>"],"mappings":"AA4DA,OAAOA,KAAI,MAAO,OAAM;AAExB,OAAOC,UAAS,MAAO,6BAA4B;AAEnD,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,EAAE;MACdC,cAAc,EAAE;IACpB;EACJ,CAAC;EACDC,UAAU,EAAE;IACRL;EACJ,CAAC;EACD,MAAMM,OAAOA,CAAA,EAAG;IACZC,OAAO,CAACC,GAAG,CAAC,SAAS;IAErBC,QAAQ,CAACC,KAAI,GAAI,oBAAmB;IAEpC,MAAMX,KAAI,CACLY,GAAG,CAAC,yBAAyB,EAC7BC,IAAI,CAACC,QAAO,IAAK;MACdN,OAAO,CAACC,GAAG,CAACK,QAAQ,CAACZ,IAAI;MAEzB,IAAI,CAACE,UAAS,GAAIU,QAAQ,CAACZ,IAAG;IAClC,CAAC;IAEL,IAAI,CAACa,UAAU,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACLC,iBAAiBA,CAACC,QAAQ,EAAE;MACxBV,OAAO,CAACC,GAAG,CAACS,QAAQ;MACpB,IAAI,CAACb,cAAa,GAAIa,QAAO;MAE7B,IAAI,CAACH,UAAU,CAAC;IACpB,CAAC;IACDA,UAAUA,CAAA,EAAG;MACT,IAAII,GAAE,GAAI,UAAS;MAEnB,IAAI,IAAI,CAACd,cAAc,EAAE;QACrBc,GAAE,IAAK,eAAc,GAAI,IAAI,CAACd,cAAc,CAACe,EAAC;MAClD;MAEApB,KAAI,CACCY,GAAG,CAACO,GAAG,EACPN,IAAI,CAACC,QAAO,IAAK;QACdN,OAAO,CAACC,GAAG,CAACK,Q