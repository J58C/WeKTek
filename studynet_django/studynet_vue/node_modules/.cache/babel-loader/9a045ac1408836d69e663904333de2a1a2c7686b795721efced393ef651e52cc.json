{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      errors: []\n    };\n  },\n  mounted() {\n    document.title = 'Log in | StudyNet';\n  },\n  methods: {\n    submitForm() {\n      console.log('submitForm');\n      axios.defaults.headers.common['Authorization'] = \"\";\n      localStorage.removeItem('token');\n      this.errors = [];\n      if (this.username === '') {\n        this.errors.push('The username is missing!');\n      }\n      if (this.password === '') {\n        this.errors.push('The password is missing!');\n      }\n      if (!this.errors.length) {\n        const formData = {\n          username: this.username,\n          password: this.password\n        };\n        axios.post('token/login/', formData).then(response => {\n          const token = response.data.auth_token;\n          this.$store.commit('setToken', token);\n          axios.defaults.headers.common['Authorization'] = \"Token \" + token;\n          localStorage.setItem('token', token);\n          this.$router.push('/dashboard/my-account');\n        }).catch(error => {\n          if (error.response) {\n            for (const property in error.response.data) {\n              this.errors.push(`${property}: ${error.response.data[property]}`);\n            }\n            console.log(JSON.stringify(error.response.data));\n          } else if (error.message) {\n            this.errors.push('Something went wrong. Please try again');\n            console.log(JSON.stringify(error));\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","errors","mounted","document","title","methods","submitForm","console","log","defaults","headers","common","localStorage","removeItem","push","length","formData","post","then","response","token","auth_token","$store","commit","setItem","$router","catch","error","property","JSON","stringify","message"],"sources":["D:\\Kampus\\Project\\Web\\Web DJKI\\studynet_django\\studynet_vue\\src\\views\\LogIn.vue"],"sourcesContent":["<template>\n  <div class=\"login\">\n    <div class=\"hero is-info\">\n      <div class=\"hero-body has-text-centered\">\n        <h1 class=\"title\">Log in</h1>\n      </div>\n    </div>\n\n    <section class=\"section\">\n        <div class=\"container\">\n            <div class=\"columns\">\n                <div class=\"column is-4 is-offset-4\">\n                    <form v-on:submit.prevent=\"submitForm\">\n                        <div class=\"field\">\n                            <label>Email</label>\n                            <div class=\"control\">\n                                <input type=\"email\" class=\"input\" v-model=\"username\">\n                            </div>\n                        </div>\n\n                        <div class=\"field\">\n                            <label>Password</label>\n                            <div class=\"control\">\n                                <input type=\"password\" class=\"input\" v-model=\"password\">\n                            </div>\n                        </div>\n\n                        <div class=\"notification is-danger\" v-if=\"errors.length\">\n                            <p\n                                v-for=\"error in errors\"\n                                v-bind:key=\"error\"\n                            >\n                                {{ error }}\n                            </p>\n                        </div>\n\n                        <div class=\"field\">\n                            <div class=\"control\">\n                                <button class=\"button is-dark\">Log in</button>\n                            </div>\n                        </div>\n                    </form>\n\n                    <hr>\n\n                    Or <router-link to=\"/sign-up\">click here</router-link> to sign up!\n                </div>\n            </div>\n        </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    data() {\n        return {\n            username: '',\n            password: '',\n            errors: []\n        }\n    },\n    mounted() {\n        document.title = 'Log in | StudyNet'\n    },\n    methods: {\n        submitForm() {\n            console.log('submitForm')\n\n            axios.defaults.headers.common['Authorization'] = \"\"\n\n            localStorage.removeItem('token')\n\n            this.errors = []\n\n            if (this.username === '') {\n                this.errors.push('The username is missing!')\n            }\n\n            if (this.password === '') {\n                this.errors.push('The password is missing!')\n            }\n\n            if (!this.errors.length) {\n                const formData = {\n                    username: this.username,\n                    password: this.password\n                }\n\n                axios\n                    .post('token/login/', formData)\n                    .then(response => {\n                        const token = response.data.auth_token\n\n                        this.$store.commit('setToken', token)\n\n                        axios.defaults.headers.common['Authorization'] = \"Token \" + token\n\n                        localStorage.setItem('token', token)\n\n                        this.$router.push('/dashboard/my-account')\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please try again')\n                            \n                            console.log(JSON.stringify(error))\n                        }\n                    })\n            }\n        }\n    }\n}\n</script>"],"mappings":";AAsDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNC,QAAQ,CAACC,KAAI,GAAI,mBAAkB;EACvC,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACTC,OAAO,CAACC,GAAG,CAAC,YAAY;MAExBX,KAAK,CAACY,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,IAAI,EAAC;MAElDC,YAAY,CAACC,UAAU,CAAC,OAAO;MAE/B,IAAI,CAACZ,MAAK,GAAI,EAAC;MAEf,IAAI,IAAI,CAACF,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACE,MAAM,CAACa,IAAI,CAAC,0BAA0B;MAC/C;MAEA,IAAI,IAAI,CAACd,QAAO,KAAM,EAAE,EAAE;QACtB,IAAI,CAACC,MAAM,CAACa,IAAI,CAAC,0BAA0B;MAC/C;MAEA,IAAI,CAAC,IAAI,CAACb,MAAM,CAACc,MAAM,EAAE;QACrB,MAAMC,QAAO,GAAI;UACbjB,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACnB;QAEAH,KAAI,CACCoB,IAAI,CAAC,cAAc,EAAED,QAAQ,EAC7BE,IAAI,CAACC,QAAO,IAAK;UACd,MAAMC,KAAI,GAAID,QAAQ,CAACrB,IAAI,CAACuB,UAAS;UAErC,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,UAAU,EAAEH,KAAK;UAEpCvB,KAAK,CAACY,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,IAAI,QAAO,GAAIS,KAAI;UAEhER,YAAY,CAACY,OAAO,CAAC,OAAO,EAAEJ,KAAK;UAEnC,IAAI,CAACK,OAAO,CAACX,IAAI,CAAC,uBAAuB;QAC7C,CAAC,EACAY,KAAK,CAACC,KAAI,IAAK;UACZ,IAAIA,KAAK,CAACR,QAAQ,EAAE;YAChB,KAAK,MAAMS,QAAO,IAAKD,KAAK,CAACR,QAAQ,CAACrB,IAAI,EAAE;cACxC,IAAI,CAACG,MAAM,CAACa,IAAI,CAAE,GAAEc,QAAS,KAAID,KAAK,CAACR,QAAQ,CAACrB,IAAI,CAAC8B,QAAQ,CAAE,EAAC;YACpE;YAEArB,OAAO,CAACC,GAAG,CAACqB,IAAI,CAACC,SAAS,CAACH,KAAK,CAACR,QAAQ,CAACrB,IAAI,CAAC;UACnD,OAAO,IAAI6B,KAAK,CAACI,OAAO,EAAE;YACtB,IAAI,CAAC9B,MAAM,CAACa,IAAI,CAAC,wCAAwC;YAEzDP,OAAO,CAACC,GAAG,CAACqB,IAAI,CAACC,SAAS,CAACH,KAAK,CAAC;UACrC;QACJ,CAAC;MACT;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}